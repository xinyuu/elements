/**
  Box is a demo element.
  \color #666
  \symbol B2
  \symmetries normal
*/
element Box2{
  Unary(4) ns;
  Void behave(){
    DebugUtils du;
    EventWindow ew;
    if(ns==0){
      ns=1;
    }
    else if(ns==1){
      ns=2;
    }
    else if(ns==2){
      ns=3;
    }
    else if(ns==3){
      ns=0;
    }
    ew.changeSymmetry((Unsigned)ns);
    du.print((Int) ns);
    ew.swap(0,1);
  }
}